    "@types/n3": "^1.0.1",
    "@types/node": "^11.9.0",
    "manual-git-changelog": "^1.0.0",
    "pre-commit": "^1.2.2",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^5.1.0",
    "typescript": "^3.1.1"
  },
  "pre-commit": [
    "build",
    "lint"
